list_numbers = [1, 3, 4, 2, 6, 5, 8, 7, 9, 10, 14, 13, 20, 21, 17, 15, 19]

def sort_asc(lst):
    for i in range(0, (len(lst) - 1)):
        if lst[i] < lst[i + 1]:
            continue
        else:
            temp = lst[i + 1]
            lst[i + 1] = lst[i]
            lst[i] = temp
            
    return lst

sort_list = sort_asc(list_numbers)
sorted_list = sort_asc(list_numbers)
